<?xml version="1.0" encoding="utf-8"?>
<settings>
	<vertex_shaders>
		<vs_deferredRPVS>"shaders/bin/vs_deferredRPVS.fxc"</vs_deferredRPVS>
		<vs_mainVS>"shaders/bin/vs_mainVS.fxc"</vs_mainVS>
		<vs_shadowMapVS>"shaders/bin/vs_shadowMapVS.fxc"</vs_shadowMapVS>
	</vertex_shaders>
	<pixel_shaders>
		<ps_deferred>"shaders/bin/ps_deferred.fxc"</ps_deferred>
		<ps_deferredRPPostLightingPS>"shaders/bin/ps_deferredRPPostLightingPS.fxc"</ps_deferredRPPostLightingPS>
		<ps_deferredRPPS>"shaders/bin/ps_deferredRPPS.fxc"</ps_deferredRPPS>
		<ps_displayDepthPS>"shaders/bin/ps_displayDepthPS.fxc"</ps_displayDepthPS>
		<ps_displayShadowMap>"shaders/bin/ps_displayShadowMap.fxc"</ps_displayShadowMap>
		<ps_edgeOutlinePS>"shaders/bin/ps_edgeOutlinePS.fxc"</ps_edgeOutlinePS>
		<ps_error>"shaders/bin/ps_error.fxc"</ps_error>
		<ps_gaussianBlurFilterPS>"shaders/bin/ps_gaussianBlurFilterPS.fxc"</ps_gaussianBlurFilterPS>
		<ps_identityFilterPS>"shaders/bin/ps_identityFilterPS.fxc"</ps_identityFilterPS>
		<ps_shadowMap>"shaders/bin/ps_shadowMap.fxc"</ps_shadowMap>
		<ps_shadowMaskDitherFilterPS>"shaders/bin/ps_shadowMaskDitherFilterPS.fxc"</ps_shadowMaskDitherFilterPS>
		<ps_shadowMaskPCFFilterPS>"shaders/bin/ps_shadowMaskPCFFilterPS.fxc"</ps_shadowMaskPCFFilterPS>
		<ps_shadowMaskPS>"shaders/bin/ps_shadowMaskPS.fxc"</ps_shadowMaskPS>
		<ps_unlit>"shaders/bin/ps_unlit.fxc"</ps_unlit>
	</pixel_shaders>
	<shader_sets>
		<error>
			<vertex>vs_mainVS</vertex>
			<pixel>ps_error</pixel>
		</error>
		<deferred_mat>
			<vertex>vs_mainVS</vertex>
			<pixel>ps_deferred</pixel>
		</deferred_mat>
		<unlit_mat>
			<vertex>vs_mainVS</vertex>
			<pixel>ps_unlit</pixel>
		</unlit_mat>
		<deferred_rp>
			<vertex>vs_deferredRPVS</vertex>
			<pixel>ps_deferredRPPS</pixel>
		</deferred_rp>
		<deferred_rp_post_lighting>
			<vertex>vs_deferredRPVS</vertex>
			<pixel>ps_deferredRPPostLightingPS</pixel>
		</deferred_rp_post_lighting>
		<csm_shadow_map>
			<vertex>vs_shadowMapVS</vertex>
			<pixel>ps_shadowMap</pixel>
		</csm_shadow_map>
		<display_shadow_map>
			<vertex>vs_deferredRPVS</vertex>
			<pixel>ps_displayShadowMap</pixel>
		</display_shadow_map>
		<csm_pcf_filter>
			<vertex>vs_deferredRPVS</vertex>
			<pixel>ps_shadowMaskPCFFilterPS</pixel>
		</csm_pcf_filter>
		<csm_dither_filter>
			<vertex>vs_deferredRPVS</vertex>
			<pixel>ps_shadowMaskDitherFilterPS</pixel>
		</csm_dither_filter>
		<shadow_mask>
			<vertex>vs_deferredRPVS</vertex>
			<pixel>ps_shadowMaskPS</pixel>
		</shadow_mask>
		<identity_filter>
			<vertex>vs_deferredRPVS</vertex>
			<pixel>ps_identityFilterPS</pixel>
		</identity_filter>
		<gauss_blur_filter>
			<vertex>vs_deferredRPVS</vertex>
			<pixel>ps_gaussianBlurFilterPS</pixel>
		</gauss_blur_filter>
		<outline_mat>
			<vertex>vs_deferredRPVS</vertex>
			<pixel>ps_edgeOutlinePS</pixel>
		</outline_mat>
	</shader_sets>
</settings>